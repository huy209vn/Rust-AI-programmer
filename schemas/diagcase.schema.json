{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Diagnostic Case",
  "type": "object",
  "properties": {
    "schema_version": {
      "const": "1"
    },
    "repo": {
      "type": "string"
    },
    "commit": {
      "type": "string"
    },
    "files": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "cargo_diag": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "tests": {
      "type": [
        "array",
        "null"
      ]
    },
    "class": {
      "type": "string"
    },
    "gold_patch": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "required": [
    "schema_version",
    "repo",
    "commit",
    "files",
    "cargo_diag",
    "class"
  ]
}