{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Project Graph Snapshot",
  "type": "object",
  "properties": {
    "schema_version": {
      "const": "1"
    },
    "project_graph_v1": {
      "type": "object"
    },
    "ra_hash": {
      "type": "string"
    },
    "symbol_ids": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "schema_version",
    "project_graph_v1",
    "ra_hash"
  ]
}